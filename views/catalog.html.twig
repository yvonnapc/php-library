<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
    </head>
    <body>
        {% if patron %}
            <ul>
                {% for book in books %}
                <li><a href="/patron/{{patron.getId}}/catalog/{{book.getId()}}">{{book.getTitle}}</a></li>
                {% endfor %}
            </ul>
        {% endif%}

        {% if librarian %}
            <ul>
                {% for book in books %}
                <li><a href="/librarian/catalog/{{book.getId()}}">{{book.getTitle}}</a></li>
                {% endfor %}
            </ul>
        {% endif %}
    </body>
</html>
